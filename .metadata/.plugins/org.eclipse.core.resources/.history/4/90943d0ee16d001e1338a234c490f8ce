#include "adc.h"

void adc_init() {
    SYSCTL_RCGCGPIO_R |= 0x2; // enable GPIOB clock
    SYSCTL_RCGCADC |= 0x1; // enable ADC0 clock

    GPIO_PORTB_AFSEL_R &= 0xEF;

    GPIO_PORTB_DEN_R &= 0xEF;

    GPIO_PORTB_DIR_R &= 0xEF;


    GPIO_PORTB_AMSEL_R


    ADCX_EMUX_R

    ADX_ACTSS_R

    ADCX_TSSEL_R

    ADCX_SSMUXX_R

    ADCX_SSCTLX_R

    ADC0_SAC_R -- hardware average




}

void adc_read(void);
